<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dynamicdrillers.collegenoticeboard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminRegistrationActivity" />
        <activity android:name=".AdminRegCollegeActivity" />
        <activity android:name=".AdminRegImgActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".FacultyRegistrationActivity" />
        <activity android:name=".FacultyDashboard" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".ShowNoticeByType"
            android:parentActivityName=".FacultyDashboard" />
        <activity
            android:name=".NoticePostShow"
            android:parentActivityName=".FacultyDashboard" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".HodRgistrationActivity" />
        <activity android:name=".StudentRegistrationActivity" />
        <activity android:name=".StudentProfileActivity" />
        <activity android:name=".EditStudentProActivity" />
        <activity android:name=".UpdateProImgActivity"></activity>
    </application>

</manifest>