<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="dynamicdrillers.collegenoticeboard.ShowNoticeByType">

    <include layout="@layout/noticetoolbar" android:id="@+id/single_notice_toolbar"></include>


    <android.support.v7.widget.RecyclerView
        android:id="@+id/noticelist_recylerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/single_notice_toolbar"
        />
</RelativeLayout>
